{
  "name": "baihub-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "start:go": "expo start",
    "android": "expo run:android",
    "android:dev": "expo run:android --variant debug",
    "android:release": "expo run:android --variant release",
    "android:apk": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "build:bundle": "npx expo export --platform android",
    "build:apk": "npm run build:bundle && npm run android:apk",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.0",
    "@react-native-firebase/analytics": "^23.5.0",
    "@react-native-firebase/app": "^23.5.0",
    "@react-native-firebase/remote-config": "^23.5.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "axios": "^1.7.9",
    "expo": "~54.0.23",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~6.0.19",
    "expo-location": "^19.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.12.5",
    "react-native-razorpay": "^2.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.6.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "@types/react": "~19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
